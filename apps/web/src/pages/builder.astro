---
import Layout from "../layouts/Layout.astro";
import { BuilderApp } from "../components/Builder";
import {WaveformData} from "@waveformr/core";

let data= await fetch('http://localhost:3000/Good Sport.dat').then(response => response.arrayBuffer())

let waveformData = WaveformData.create(data).toJSON()

let sample = {
  name: "Good Sport",
  data: waveformData
} as const

---

<Layout title="Builder | Waveformr">
  <BuilderApp client:load sample={sample} />
</Layout>
